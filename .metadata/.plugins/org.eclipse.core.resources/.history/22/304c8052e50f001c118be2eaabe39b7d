package basic;
import java.util.Scanner;
public class Test {

	public static void main(String[] args) {
		
	int n;
	Scanner s=new Scanner(System.in);
	System.out.println("Enter ur limit:");
	n=s.nextInt();
	for(int i=1;i<=n;i++)
	{
		System.out.println(i);
		for(int j=i+1;j<=n;j++)
		{
		System.out.println(j);
		}
	
	}
	}
	}


